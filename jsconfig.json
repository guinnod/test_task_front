{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~api/*": [
                "src/api/*"
            ],
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~hooks/*": [
                "src/hooks/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~/*": [
                "src/*"
            ],
            "api/*": [
                "src/api/*"
            ],
            "assets/*": [
                "src/assets/*"
            ],
            "components/*": [
                "src/components/*"
            ],
            "hooks/*": [
                "src/hooks/*"
            ],
            "store/*": [
                "src/store/*"
            ],
            "/*": [
                "src/*"
            ],
            "@api/*": [
                "src/api/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@store/*": [
                "src/store/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@services/*": [
                "src/services/*"
            ],
            "@context/*": [
                "src/context/*"
            ],
            "@tests/*": [
                "src/tests/*"
            ]
        }
    }
}